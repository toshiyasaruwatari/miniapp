<div class="row">
  <div class="col s9">
    <div class="row">
      <div class="input-field col s9">
        <%= form_for(@blog, :url => {:controller => :blogs, :action => :create}) do |f| %>
          <%= f.label :タイトル %><br />
          <%= f.text_field :title %><br />
          <%= f.label :本文 %><br />
          <%= f.text_field :text %><br />
          <%= f.submit "投稿", :class=>"btn" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
